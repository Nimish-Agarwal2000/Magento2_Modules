    <resources>
      <resource id="Magento_Backend::admin">
        <resource id="Dotsquares_Marketplace::main_menu" title="Dotsquares Marketplace" sortOrder="20">
          <resource id="Dotsquares_Marketplace::settings" title="Configurations" sortOrder="4" />
